{
  "name": "G2",
  "version": "7.0.0",
  "description": "Developement enviroment for the G2 Project.",
  "main": "ui/app.js",
  "scripts": {
    "install-deps": "npm install && npm i -g webpack webpack-dev-server",
    "preinstall": "node ./BuildTools/Scripts/checkVersions",
    "analyze": "webpack --config ./BuildTools/webpack.config.babel.js --profile --json > stats.json",
    "analyzer": "webpack --config ./BuildTools/webpack.config.babel.js --profile --json | webpack-bundle-size-analyzer",
    "lint": "eslint --format ./node_modules/eslint-friendly-formatter/index.js --ext .js --ext .jsx ./Modules/** ./App/** ./SharedComponents/** || exit 0 ",
    "lint:fix": "eslint --format ./node_modules/eslint-friendly-formatter/index.js --ext .js --ext .jsx ./Modules/** ./App/** ./SharedComponents/** --fix || exit 0 ",
    "replace": "node ./BuildTools/Clean",
    "prod": "cross-env NODE_ENV=production webpack --config ./BuildTools/webpack.config.vendors.babel.js --progress --colors && webpack --config ./BuildTools/webpack.config.babel.js --progress --colors",
    "vendors": "cross-env NODE_ENV=development BUILDLIKEPROD=true webpack --config ./BuildTools/webpack.config.vendors.babel.js --colors --display-error-details --progress",
    "watch": "cross-env NODE_ENV=development BUILDLIKEPROD=true UV_THREADPOOL_SIZE=100 webpack --config ./BuildTools/webpack.config.babel.js --watch --colors --progress",
    "postwatch": "npm run prod",
    "build": "npm run vendors && cross-env UV_THREADPOOL_SIZE=100 NODE_ENV=development BUILDLIKEPROD=true webpack --config ./BuildTools/webpack.config.babel.js --colors --display-reasons --display-error-details --progress --profile",
    "predev:vendors:build:watch": "npm run vendors && start cmd.exe @cmd /k npm run watch",
    "dev:vendors:build:watch": "npm run dev",
    "dev": "cross-env NODE_ENV=development UV_THREADPOOL_SIZE=100 LINT_CODE=true node ./BuildTools/Servers/Proxy",
    "dev:turbo": "cross-env NODE_ENV=development UV_THREADPOOL_SIZE=100 TURBO=true node ./BuildTools/Servers/Proxy",
    "dev:offline": "node ./BuildTools/Servers/Offline",
    "kill": "taskkill /IM node.exe -F",
    "prestart": "npm run vendors && start cmd.exe @cmd /k npm run watch",
    "start": "npm run dev:turbo",
    "go": "cross-env NODE_ENV=development UV_THREADPOOL_SIZE=100 webpack --config ./BuildTools/webpack.config.babel.js --watch --progress --profile --display-modules"
  },
  "private": "true",
  "author": "G2",
  "license": "MIT",
  "dependencies": {
    "axios": "0.9.1",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "6.7.2",
    "bourbon": "4.2.6",
    "classnames": "2.2.3",
    "cross-env": "1.0.6",
    "estraverse-fb": "^1.3.1",
    "history": "2.0.1",
    "lodash": "4.6.1",
    "mixin-decorator": "1.0.5",
    "normalize.css": "3.0.3",
    "postal": "1.0.8",
    "postal.diagnostics": "0.7.4",
    "postal.federation": "0.5.3",
    "postal.xframe": "0.5.0",
    "react": "^15.0.0-rc.2",
    "react-addons-css-transition-group": "0.14.7",
    "react-addons-pure-render-mixin": "0.14.7",
    "react-dom": "^15.0.0-rc.2",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-toolbox": "0.14.2",
    "redux": "3.3.1",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.0.1"
  },
  "devDependencies": {
    "apicache": "0.0.12",
    "autoprefixer": "6.3.3",
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.7.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-syntax-decorators": "6.5.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "7.2.0",
    "body-parser": "1.15.0",
    "browser-sync": "2.11.1",
    "bunyan": "1.8.0",
    "compression": "1.6.1",
    "connect-logger": "0.0.1",
    "cookie-parser": "1.4.1",
    "css-loader": "0.23.1",
    "debug": "2.2.0",
    "del": "2.2.0",
    "eslint": "2.4.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-friendly-formatter": "1.2.2",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gulp": "3.9.1",
    "gulp-start-process": "1.1.1",
    "gulp-util": "3.0.7",
    "happypack": "1.1.3",
    "html-loader": "0.4.3",
    "http-proxy-middleware": "0.12.0",
    "json-loader": "0.5.4",
    "morgan": "1.7.0",
    "node-api-cache-proxy": "0.6.3",
    "node-sass": "3.4.2",
    "postcss": "5.0.19",
    "postcss-loader": "0.8.2",
    "react-loader": "2.2.0",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "react-transform-log-render": "1.0.0",
    "redbox-react": "1.2.2",
    "redux-devtools": "3.1.1",
    "redux-devtools-diff-monitor": "4.0.1",
    "redux-devtools-dispatch": "2.0.1",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-filterable-log-monitor": "0.6.1",
    "redux-devtools-log-monitor": "1.0.5",
    "redux-import-export-monitor": "0.2.0",
    "redux-logger": "2.6.1",
    "redux-slider-monitor": "1.0.2",
    "redux-unhandled-action": "1.1.0",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.0",
    "toolbox-loader": "0.0.3",
    "vs-fix-sourcemaps": "0.1.7",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.10.0",
    "webpack-notifier": "1.3.0"
  }
}
